{
  "name": "Apollo-Server-GraphQL-server",
  "version": "1.0.0",
  "description": "A lightweight GraphQL API server",
  "keywords": [
    "express",
    "API",
    "mongoose"
  ],
  "license": "MIT",
  "url": "https://github.com/tomanagle/Apollo-Server-GraphQL-starter",
  "author": "Tom Nagle",
  "email": "tomanagle@gmail.com",
  "scripts": {
    "lint": "eslint --ext .js,.jsx app",
    "start": "npm run build && cross-env NODE_ENV=production pm2 start process.json",
    "dev": "cross-env NODE_ENV=development && nodemon ./node_modules/babel-cli/bin/babel-node.js ./app/app.js",
    "build": "babel ./app -d dist --presets es2015,stage-2",
    "test": "nyc --reporter=html mocha --compilers js:babel-core/register **/\"specs/*.spec.js\"",
    "setup": "node ./node_modules/babel-cli/bin/babel-node.js app/setup.js"
  },
  "dependencies": {
    "@babel/core": "^7.1.0",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.0.6",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-node8": "^1.2.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "deep-clean": "^1.0.2",
    "dotenv": "^5.0.1",
    "ejs-promise": "^0.3.3",
    "express": "~4.15.5",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.5",
    "jsonwebtoken": "^8.1.0",
    "jsonwebtoken-promisified": "^1.0.3",
    "loggaroo": "^1.1.3",
    "mailgun-js": "^0.20.0",
    "method-override": "^2.3.10",
    "moment": "^2.22.2",
    "mongoose": "^5.2.9",
    "mongoose-field-remover": "0.0.3",
    "mongoose-permalink": "^2.0.0",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "~1.9.0",
    "njwt": "^0.4.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "shortid": "^2.2.12",
    "validator": "^9.2.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.1.0",
    "babel-cli": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "cross-env": "^5.1.3",
    "eslint": "^3.19.0",
    "flow-bin": "^0.81.0",
    "mocha": "^3.2.0",
    "nyc": "^11.4.1"
  }
}
