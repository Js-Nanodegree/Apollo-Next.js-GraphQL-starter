{
  "name": "Apollo-Server-GraphQL-server",
  "version": "1.0.0",
  "description": "A lightweight GraphQL API server",
  "keywords": [
    "express",
    "API",
    "mongoose"
  ],
  "license": "MIT",
  "url": "https://github.com/tomanagle/Apollo-Server-GraphQL-starter",
  "author": "Tom Nagle",
  "email": "tomanagle@gmail.com",
  "scripts": {
    "lint": "eslint --ext .js,.jsx app",
    "start": "npm run build && cross-env NODE_ENV=production pm2 start process.json",
    "dev": "nodemon",
    "tsc": "tsc",
    "build": "yarn tsc && cpx 'app/graphql/schema/*.graphql' 'dist/graphql/schema'",
    "test": "nyc --reporter=html mocha --compilers js:babel-core/register **/\"specs/*.spec.js\"",
    "setup": "node ./node_modules/babel-cli/bin/babel-node.js app/setup.js"
  },
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@types/cookie-parser": "^1.4.1",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.4.8",
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cpx": "^1.5.0",
    "debug": "~2.6.9",
    "deep-clean": "^1.0.2",
    "dotenv": "^5.0.1",
    "ejs-promise": "^0.3.3",
    "express": "~4.15.5",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.5",
    "jsonwebtoken": "^8.1.0",
    "jsonwebtoken-promisified": "^1.0.3",
    "loggaroo": "^1.1.3",
    "mailgun-js": "^0.20.0",
    "method-override": "^2.3.10",
    "moment": "^2.22.2",
    "mongoose": "^5.2.9",
    "mongoose-field-remover": "0.0.3",
    "mongoose-permalink": "^2.0.0",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "~1.9.0",
    "njwt": "^0.4.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "shortid": "^2.2.12",
    "validator": "^9.2.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.13",
    "@types/method-override": "^0.0.31",
    "@types/mongoose": "^5.3.9",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.19",
    "@types/validator": "^10.9.0",
    "cross-env": "^5.1.3",
    "eslint": "^3.19.0",
    "jest": "^24.0.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "typescript": "^3.2.4"
  }
}
