{
  "name": "apollo-graphql-server",
  "version": "1.0.0",
  "description": "A lightweight GraphQL API server",
  "keywords": [
    "express",
    "API",
    "mongoose"
  ],
  "license": "MIT",
  "url": "https://github.com/tomanagle/Apollo-Server-GraphQL-starter",
  "author": "Tom Nagle",
  "email": "tomanagle@gmail.com",
  "scripts": {
    "lint": "eslint --ext .js,.ts ./app",
    "lint:fix": "eslint --ext .js,.ts ./app --fix",
    "deploy": "npm run build && cross-env NODE_ENV=production pm2 start process.json",
    "dev": "nodemon",
    "tsc": "tsc",
    "build": "yarn tsc && cpx 'app/graphql/schema/*.graphql' 'dist/graphql/schema'",
    "test": "jest",
    "test:watch": "jest --watch",
    "setup": "node ./node_modules/babel-cli/bin/babel-node.js app/setup.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.4.8",
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cpx": "^1.5.0",
    "debug": "~2.6.9",
    "deep-clean": "^1.0.2",
    "dotenv": "^5.0.1",
    "ejs-promise": "^0.3.3",
    "express": "~4.16.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.5",
    "jsonwebtoken": "^8.1.0",
    "jsonwebtoken-promisified": "^1.0.3",
    "loggaroo": "^1.1.3",
    "mailgun-js": "^0.20.0",
    "method-override": "^2.3.10",
    "moment": "^2.22.2",
    "mongoose": "^5.2.12",
    "mongoose-field-remover": "0.0.3",
    "mongoose-permalink": "^2.0.0",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.9.0",
    "njwt": "^1.0.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "shortid": "^2.2.12",
    "validator": "^9.2.0",
    "winston": "^3.0.0",
    "snyk": "^1.161.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.3",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.0.12",
    "@types/method-override": "^0.0.31",
    "@types/mongoose": "^5.3.9",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.19",
    "@types/validator": "^10.9.0",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "cross-env": "^5.1.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "mock-req-res": "^1.1.0",
    "prettier": "^1.17.0",
    "sinon": "^7.3.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.2"
  }
}
